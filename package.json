{
    "name": "vircadia-world-sdk-ts",
    "type": "module",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "browser": "./dist/browser/index.js",
            "bun": "./dist/bun/index.js"
        }
    },
    "sideEffects": false,
    "dependencies": {
        "@vueuse/core": "^13.1.0",
        "console-log-colors": "latest",
        "eight-colors": "latest",
        "jsonwebtoken": "^9.0.2",
        "lodash-es": "^4.17.21",
        "mobx": "latest",
        "postgres": "latest",
        "tmp": "^0.2.3",
        "typescript": "latest",
        "vue": "^3.5.13",
        "zod": "latest",
        "idb": "^8.0.2"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "latest",
        "@types/node": "^22.13.10",
        "@types/tmp": "^0.2.6",
        "@types/lodash-es": "^4.17.12",
        "bun-types": "latest"
    },
    "scripts": {
        "format": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
        "lint": "eslint './**/*.{ts,tsx,js,jsx}'",
        "build:browser": "bun run build.browser.js",
        "build:bun": "bun run build.bun.js",
        "build:types": "tsc --emitDeclarationOnly",
        "build": "bun run build.js",
        "pack": "cd package && npm pack ../",
        "dev": "bun --watch build.js"
    }
}
